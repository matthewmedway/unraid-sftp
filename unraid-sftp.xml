<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>sftp</Name>
  <Date>2019-03-10</Date>
  <Category>Tools / Utilities:System</Category>
  <Support>https://github.com/mysecureshell/mysecureshell/issues</Support>
  <Description>for unraid</Description>
  <Project>https://github.com/mysecureshell/mysecureshell</Project>
  <Overview>Community app for sftp running on unraid</Overview>
  <Registry>https://hub.docker.com/r/drbrocoli/unraid-sftp/</Registry>
  <Repository>drbrocoli/unraid-sftp</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment>
    <Variable>
      <Name>username</Name>
      <Value>The username of your new sftpuser</Value>
    </Variable>
    <Variable>
      <Name>password</Name>
      <Value>The ssh key for your new user</Value>
    </Variable>
    <Variable>
      <Name>publickey</Name>
      <Value>The password for your new user</Value>
    </Variable>
  </Environment>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>22222</HostPort>
        <ContainerPort>22</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/vault</HostDir>
      <ContainerDir>/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <ExtraParams></ExtraParams>
  <DonateText>Donations</DonateText>
  <DonateLink>https://github.com/mysecureshell/mysecureshell</DonateLink>
  <BaseImage>debidebian:stable</BaseImage>
</Container>